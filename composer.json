{
    "name": "danielmlozano/laravel-conekta",
    "description": "Laravel package that provides a simple, easy-to-use interface to Conekta Payment services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Mendoza Lozano",
            "email": "daniel@danielmlozano.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "nesbot/carbon": "^2.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "conekta/conekta-php": "^4.3"
    },
    "require-dev":{
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload":{
        "psr-4": {
            "Danielmlozano\\LaravelConekta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Danielmlozano\\LaravelConekta\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danielmlozano\\LaravelConekta\\LaravelConektaServiceProvider"
            ]
        }
    }
}
